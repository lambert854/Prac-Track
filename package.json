{
  "name": "the-prac-track",
  "version": "0.1.0",
  "private": true,
  "description": "Social Work Practicum Education Management System",
  "scripts": {
    "dev": "next dev --turbopack",
    "dev:mobile": "next dev --turbopack --hostname 0.0.0.0 --port 3000",
    "dev:https": "node server.js",
    "build": "next build --turbopack",
    "postinstall": "prisma generate",
    "start": "next start",
    "start:mobile": "next start --hostname 0.0.0.0 --port 3000",
    "lint": "eslint",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:seed": "tsx prisma/seed.ts",
    "db:studio": "prisma studio",
    "setup:mobile": "npm run db:generate && npm run db:push && npm run db:seed",
    "setup:mobile:auto": "node setup-mobile.js"
  },
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "@hookform/resolvers": "^5.2.2",
    "@prisma/client": "^6.16.3",
    "@react-pdf/renderer": "^4.3.1",
    "@tailwindcss/postcss": "^4.1.14",
    "@tanstack/react-query": "^5.90.2",
    "@tanstack/react-query-devtools": "^5.90.2",
    "bcryptjs": "^3.0.2",
    "next": "15.5.4",
    "next-auth": "^4.24.11",
    "prisma": "^6.16.3",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.63.0",
    "resend": "^6.1.2",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.5.4",
    "tailwindcss": "^4",
    "tsx": "^4.20.6",
    "typescript": "^5"
  }
}