{
  "name": "the-prac-track",
  "version": "0.1.0",
  "private": true,
  "description": "Social Work Practicum Education Management System",
  "scripts": {
    "kill-port": "npx kill-port 3000",
    "dev": "npm run kill-port && next dev --port 3000",
    "dev:mobile": "npm run kill-port && next dev --turbopack --hostname 0.0.0.0 --port 3000",
    "dev:https": "node server.js",
    "build": "prisma generate && next build",
    "build:check": "npm run type-check && npm run lint && npm run build",
    "dev:check": "node dev-with-checks.js",
    "type-check": "tsc --noEmit",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "postinstall": "prisma generate",
    "start": "next start --port 3000",
    "start:mobile": "next start --hostname 0.0.0.0 --port 3000",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:seed": "tsx prisma/seed.ts",
    "db:studio": "prisma studio",
    "setup:mobile": "npm run db:generate && npm run db:push && npm run db:seed",
    "setup:mobile:auto": "node setup-mobile.js",
    "prisma:migrate": "prisma migrate deploy",
    "prisma:seed": "tsx prisma/seed.ts",
    "db:init": "npm run prisma:migrate && npm run prisma:seed"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "@hookform/resolvers": "^5.2.2",
    "@prisma/client": "^6.16.3",
    "@react-pdf/renderer": "^4.3.1",
    "@tailwindcss/postcss": "^4.1.14",
    "@tanstack/react-query": "^5.90.2",
    "@tanstack/react-query-devtools": "^5.90.2",
    "@vercel/analytics": "^1.5.0",
    "@vercel/blob": "^2.0.0",
    "autoprefixer": "^10.4.21",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^3.0.2",
    "next": "15.5.4",
    "next-auth": "^4.24.11",
    "prisma": "^6.16.3",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.63.0",
    "resend": "^6.1.2",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "cross-env": "^10.1.0",
    "cross-env-shell": "^7.0.3",
    "eslint": "^9",
    "eslint-config-next": "15.5.4",
    "kill-port": "^2.0.1",
    "tailwindcss": "^4.1.14",
    "tsx": "^4.20.6",
    "typescript": "^5"
  }
}
